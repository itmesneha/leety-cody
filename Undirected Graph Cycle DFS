from collections import deque
class Solution:
	def isCycle(self, n, edges):
	    adj = [[] for _ in range(n)]
	    for u,v in edges:
	        adj[u].append(v)
	        adj[v].append(u)
	   # print('adj:', adj)
	    visited = [False] * n
	    
	    def dfs(u, parent):
	        if visited[u]:
	            return True
	        visited[u] = True
	        for v in adj[u]:
	            if not visited[v]:
	                if dfs(v, u):
	                    return True
	            elif v != parent:
	                return True
	                    
	        return False
	    
	    # parse all individual nodes
	    
	    for u in range(n):
	        if dfs(u, -1):
	            return True
	    return False
	        
	    
		
